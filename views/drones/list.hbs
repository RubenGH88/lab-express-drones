<h2>Available drones</h2>
<br>

 {{#each dron}}

   
    <h4>Name: {{this.name}}</h4>
    <h4>Propellers: {{this.propellers}}</h4>
    <h4>maxSpeed: {{this.maxSpeed}}</h4>
    <a href="/drones/{{this.id}}/edit">Edit</a>
     <form action="/drones/{{id}}/delete" method="POST">
        <button>Delete</button>
      </form>
    <br>


  {{/each}}